<h1
  class="jumbotron jumbotron-fluid"
  style="color:rgb(214, 206, 206); border:black solid 2px; border-radius: 6px; width:720px; background-color:rgb(51, 50, 50);margin-left:380px; margin-top:10px; padding-left:10px; padding-left:25px"
>NEW TOKEN ON-CHAIN ETH LISTING</h1>

<div class="container">
  <div class="row">
    <div class="col">
      <div class="contract-table-wrapper">
        <table class="contract-table">
          <thead>
            <tr>
              <th>Smart Contract</th>
            </tr>
          </thead>
          <tbody>
            {{#each clearToken}}
              <tr>
                <td>CA: {{this.CA}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
    <div class="col">
      <img
        src="{{imgbot}}"
        alt=""
        class="img-thumbnail"
        style="height: auto; width: auto"
      />
      {{#if botcheck}}
        <div
          class="form-container"
          style="width: 300px; margin-left:110px;border:black solid 2px; border-radius: 6px;  background-color:rgb(51, 50, 50)"
        >
          <form id="botForm" action="/botCheck" method="get">
            <button
              class="btn btn-outline-secondary"
              style=" background-color:rgb(239, 235, 235)"
            >CALL BOT CHECK ðŸ¤–</button>
          </form>
        </div>
      {{/if}}
    </div>
  </div>
</div>
<br />
<br />

<div class="trending-table-container">
  <h2
    class="header jumbotron jumbotron-fluid"
    style="color:rgb(214, 206, 206); border:black solid 2px; border-radius: 6px; width:550px; background-color:rgb(51, 50, 50); margin-left:420px"
  >TRENDING OFF-CHAIN ALT COIN</h2>
  <div
    class="form-container"
    style="color:rgb(214, 206, 206); border:black solid 2px; border-radius: 6px; width:400px; background-color:rgb(51, 50, 50); height:80px; margin-left:500px"
  >
    <form id="botForm" action="/botTrending/topPump" method="get">
      <button
        class="btn btn-outline-secondary"
        style=" background-color:rgb(239, 235, 235)"
      >CALL COMMAND NOTIFICATIONS ðŸ¤–</button>
    </form>
  </div>
  {{#if call}}
    <div
      class="form-container"
      style="color:rgb(214, 206, 206); border:black solid 2px; border-radius: 6px; width:400px; background-color:rgb(51, 50, 50); height:80px; margin-left:500px"
    >
      <form id="botForm" action="/botTrending" method="get">
        <button
          class="btn btn-outline-secondary"
          style=" background-color:rgb(239, 235, 235)"
        >BACK NEW TOKEN ETH LISTING ðŸ¤–</button>
      </form>
    </div>
  {{/if}}

  <table class="trending-table">
    <thead>
      <tr>
        <th>Rank</th>
        <th>Icon</th>
        <th>Name Token</th>
        <th>Symbol</th>
        <th>Current Price</th>
        <th>Command Notification</th>
      </tr>
    </thead>
    <tbody>
      {{#if topPump}}
        {{#each topPump}}
          <tr>
            <td>{{db.market_cap_rank}}</td>
            <td><img
                src="{{db.image}}"
                alt="Token Image"
                class="token-image"
              /></td>
            <td>{{db.name}}</td>
            <td>{{db.symbol}}</td>
            <td>{{db.current_price}}$</td>
            <td>{{tinhieu}}</td>
          </tr>
        {{/each}}
      {{else}}
        <tr>
          <td colspan="5">No Information Token</td>
        </tr>
      {{/if}}
    </tbody>
  </table>
</div>
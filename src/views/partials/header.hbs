            <a
              href="#"
              class="nav-link dropdown-toggle h a"
              id="loginDropdown"
              role="button"
              data-bs-toggle="dropdown"
              aria-expanded="false"
            >Login</a>
            <div
              class="dropdown-menu dropdown-menu-end"
              aria-labelledby="loginDropdown"
            >
              <form action="/login" method="post" class="p-3">
                <div class="mb-3" style="width: 270px;">
                  <label for="Email" class="form-label">Email</label>
                  <input
                    type="email"
                    id="Email"
                    name="Email"
                    class="form-control"
                    placeholder="Email"
                    required
                  />
                </div>
                <div class="mb-3">
                  <label for="PassWord" class="form-label">Password</label>
                  <input
                    type="password"
                    id="PassWord"
                    name="PassWord"
                    class="form-control"
                    placeholder="Password"
                    required
                  />
                </div>
                <button
                  type="submit"
                  class="btn btn-primary mt-3"
                >Login</button>
              </form>
            </div>
          </li>
          <li class="nav-item"><a
              href="/rg"
              class="nav-link h a"
            >Register</a></li>

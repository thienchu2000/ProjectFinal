        <li class="nav-item"><a href="/news" class="nav-link h a"> NEWS</a></li>
        <li class="nav-item"><a href="/order" class="nav-link h a"><svg
              xmlns="http://www.w3.org/2000/svg"
              width="22"
              height="22"
              fill="currentColor"
              class="bi bi-cart4"
              viewBox="0 0 16 16"
            >
              <path
                d="M0 2.5A.5.5 0 0 1 .5 2H2a.5.5 0 0 1 .485.379L2.89 4H14.5a.5.5 0 0 1 .485.621l-1.5 6A.5.5 0 0 1 13 11H4a.5.5 0 0 1-.485-.379L1.61 3H.5a.5.5 0 0 1-.5-.5M3.14 5l.5 2H5V5zM6 5v2h2V5zm3 0v2h2V5zm3 0v2h1.36l.5-2zm1.11 3H12v2h.61zM11 8H9v2h2zM8 8H6v2h2zM5 8H3.89l.5 2H5zm0 5a1 1 0 1 0 0 2 1 1 0 0 0 0-2m-2 1a2 2 0 1 1 4 0 2 2 0 0 1-4 0m9-1a1 1 0 1 0 0 2 1 1 0 0 0 0-2m-2 1a2 2 0 1 1 4 0 2 2 0 0 1-4 0"
              />
            </svg></a></li>
        {{#if admin}}
          <li class="nav-item dropdown" id="dropdown">
            <a class="nav-link h a">ADMIN DASHBOARD</a>
            <div
              class="nav-dropdown-custom border rounded"
              style="border: 2px solid rgb(28, 26, 26);"
            >
              <a href="/admin/getOrder" class="nav-link h">Find Order</a>
              <a href="/admin/createUser" class="nav-link h">Create User</a>
              <a href="/admin/findNews" class="nav-link h">Find News</a>
              <a href="/admin/inForBot" class="nav-link h">Control Panel</a>
              <a href="/admin" class="nav-link h">User Management</a>

            </div>
          </li>
        {{/if}}
        {{#if manager}}
          <li class="nav-item dropdown" id="dropdown">
            <a class="nav-link h a">MANAGER DASHBOARD</a>
            <div
              class="nav-dropdown-custom border rounded"
              style="border: 2px solid rgb(28, 26, 26);"
            >
              <a href="/manager" class="nav-link h">Create News</a>
              <a href="/manager/sendEmail" class="nav-link h">Create Email send
                User
              </a>
              <a href="/manager/managerData" class="nav-link h">Find News and
                Update</a>
            </div>
          </li>
        {{/if}}
        {{#if User}}
          <li class="nav-item dropdown" id="dropdown">
            <a class="nav-link h a">STAKING</a>

            <div
              class="nav-dropdown-custom border rounded p-3"
              style="border: 2px solid rgb(28, 26, 26); background-color: white; width: 240px; padding-right: 150px;"
            >
              <p
                style="font-size: 18px; color: #4CAF50; font-weight: bold; margin: 10px 0;"
              >
                Staking:
                <span id="percent">{{percent}}%</span>
              </p>
              <p style="font-size: 16px; color: #555; margin-bottom: 10px;">

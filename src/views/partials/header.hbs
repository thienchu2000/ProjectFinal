                <span id="percent">{{percent}}%</span>
                of the profit.
              </p>
              <form>
                <div class="mb-3">
                  <label class="form-label">Amount MCTK</label>
                  <input
                    type="number"
                    id="staking"
                    name="staking"
                    class="form-control"
                    required
                  />
                  <label class="form-label">Days Staking</label>
                  <input
                    type="number"
                    id="day"
                    name="day"
                    class="form-control"
                    required
                  />
                </div>
                <button
                  type="button"
                  onclick="sta()"
                  class="btn btn-dark w-100"
                >Call Wallet</button>
              </form>
            </div>
          </li>
          <li class="nav-item dropdown" id="dropdown">
            <a class="nav-link">
              <img
                src="/{{Image}}"
                alt="..."
                style="height: 70px; width: 70px; border:2px solid rgb(171, 163, 163); border-radius:90000px; "
              /></a>
            <div
              class="c border rounded"
              style="border: 2px solid rgb(28, 26, 26);"
            >
              <p
                class="nav-item a"
                style="color: rgb(25, 155, 225);"
              >{{Name}}</p>
              <a href="/orderr" class="nav-link h"> Information Orders</a>
              <a href="/findUser" class="nav-link h">Information User</a>
              <a href="/logout" class="nav-link h">Logout</a>
            </div>
          </li>
        {{else}}
